{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "LogisticMeterName": "Logistic.Delivery",
  //LOGGING
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "outputTemplate": "{Timestamp} [{Level}] {Message}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ]
  },
  "ZIPKIN_URL": "http://localhost:9411",
  "APPLICATIONINSIGHTS_CONNECTION_STRING": "InstrumentationKey=4311f5e5-e794-43c4-870c-36143608fa26;IngestionEndpoint=https://francecentral-1.in.applicationinsights.azure.com/;LiveEndpoint=https://francecentral.livediagnostics.monitor.azure.com/;ApplicationId=0ba4f75d-73af-464f-a803-dc3fa87c24d1",
  "JAEGER_URL": "http://localhost:4317",
  "OLTP_ENDPOINT": "http://otel-collector-svc.opentelemetry.svc.cluster.local:4318"
}